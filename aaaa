
for i,v in next,workspace.Builds:GetChildren() do
    if v.Name ~= "Chest" then continue end
        v.Name = "Looped"
        for i,v in v:GetDescendants() do
            if v:IsA("Part") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                task.wait(.25)
                break
            end
        end
    break
end

task.wait(1)
local a = game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Chests")
game:GetService("RunService").Heartbeat:Connect(function()
a:FireServer("RequestChests")
end)

workspace.Builds.ChildAdded:Connect(function(v)
if v.Name ~= "Chest" then return end
local chest = workspace.Builds:WaitForChild("Chest")
fireproximityprompt(chest.Prop.Chest.Lid.Main.Open)
chest.Parent = nil
end)
